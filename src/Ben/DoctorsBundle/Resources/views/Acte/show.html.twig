{% extends "BenDoctorsBundle::layout.html.twig" %}
{% block title %}
    Tableau de bord | {{ parent() }}
{% endblock %}
{% block body %}
    <h1>Acte</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ entity.date|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('acte') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('acte_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
